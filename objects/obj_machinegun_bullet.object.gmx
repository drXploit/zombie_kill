          <argument>
            <kind>1</kind>
            <string>/// Initialize the bullet.
global.instances++;

// Variables

damage = 25;        // base damage
critdamage = 0;     // critical hit additional damage

/// crit hit
crit1 = irandom(obj_player.luck);
crit2 = irandom(5);

if (crit1 == crit2) critdamage = irandom_range(70, 100);
damage += critdamage;

bullet_speed = 15;

//  Move towards mouse
move_towards_point(obj_sight.x, obj_sight.y, bullet_speed);

image_angle = direction;

// For ricochet detection
origDir = direction;

// Eject shell casing
instance_create(x-lengthdir_x(global.weapon_array[global.weapon,4],image_angle),y-lengthdir_y(global.weapon_array[global.weapon,4],image_angle),obj_ak47_casing);

// Play machinegun bullet sound
audio_play_sound(snd_ak47, 8, false);

