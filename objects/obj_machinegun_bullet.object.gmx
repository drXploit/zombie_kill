    if (image_alpha == 1)
    {
        // Play casing sound
        if (sprite_index != spr_shotgun_pellet) || (sprite_index != spr_bazooka_bullet)
        audio_play_sound(choose(snd_ricochet1,
                            snd_ricochet2,
                            snd_ricochet3,
                            snd_ricochet4,
