            <string>/// Manage XP
if (instance_exists(obj_player)) {
    if (global.xp &gt;= global.levelxp) {
        global.xp = global.xp - global.levelxp;
        global.level++;
        global.levelxp *= 2;
        scr_textfloat(obj_player.x, obj_player.y, c_fuchsia, "+Level (" + string(global.level) + ")");
