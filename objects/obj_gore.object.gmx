            <kind>1</kind>
            <string>/// After enemy death, manage gore/reanimation
global.instances++;

tendancy = -1;
aliveModifier = -1;

spawnEnemy = -1;

alive = false;

gore_life = room_speed*10;

head = instance_create(x,y,obj_gore_head);
torso = instance_create(x,y,obj_gore_torso);

hand1 = instance_create(x,y,obj_gore_hand);
hand2 = instance_create(x,y,obj_gore_hand);

foot1 = instance_create(x,y,obj_gore_foot);
