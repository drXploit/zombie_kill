    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Gore management system step

if (gore_life &gt; 0) gore_life--;

if (gore_life &lt;= 0) &amp;&amp; (!alive)
    instance_destroy();

if (alive)
{
if (!instance_exists(head))    &amp;&amp;
    (!instance_exists(torso))    &amp;&amp;
    (!instance_exists(foot1))    &amp;&amp;
    (!instance_exists(foot2))    &amp;&amp;
    (!instance_exists(hand1))    &amp;&amp;
    (!instance_exists(hand2))
    {
       // effect_create_above(ef_smoke,x,y,5,c_red);
        instance_destroy();
    }
    if (head.attached) &amp;&amp;
        (torso.attached) &amp;&amp;
        (foot1.attached) &amp;&amp;
        (foot2.attached) &amp;&amp;
        (hand1.attached) &amp;&amp;
        (hand2.attached) 
        {
            scr_textfloat(torso.x,torso.y-sprite_height,c_purple,"Reanimated!"); 
       //     effect_create_above(ef_smokeup,torso.x,torso.y,1,c_purple);
            spawnEnemy = instance_create(torso.x,torso.y,obj_enemy)
            with (spawnEnemy)
            {
                maxhp = 300;
                hp = 300;
                damage *= 3;
                xp *= 5;
                blender = 255;
                image_xscale *= 1.1;
                image_yscale *= 1.1;
            }
            do
            {
                spawnEnemy.x += choose(-10,10); //random(room_width);
                spawnEnemy.y += choose(-10,10); //random(room_height);
                spawnEnemy.mask_index = spr_zombie1;
                with (spawnEnemy)
                {
                    collision = place_meeting(x, y, obj_wall_collision);
                }
            }
            until (spawnEnemy.collision == 0);
            
            
            // Play sound
            choose(audio_play_sound(snd_zombie_alive1, 8, false),
                    audio_play_sound(snd_zombie_alive2, 8, false),
                    audio_play_sound(snd_zombie_alive3, 8, false),
                    audio_play_sound(snd_zombie_alive4, 8, false));
            
            with (head) instance_destroy();
            with (torso) instance_destroy();
            with (hand1) instance_destroy();        
            with (hand2) instance_destroy();
            with (foot1) instance_destroy();
            with (foot2) instance_destroy();

            global.zombiesAttaching--;    

            instance_destroy();
        }
        
}
if (aliveModifier != -1)
{
    alive = true;
    global.zombiesAttaching++;
    head.aliveModifier = torso;
    torso.aliveModifier = head;
    foot1.aliveModifier = torso;
    foot2.aliveModifier = torso;
    hand1.aliveModifier = torso;
    hand2.aliveModifier = torso;
    aliveModifier = -1;
}

if (tendancy != -1)
{
    head.tendancy = tendancy;
    torso.tendancy = tendancy;
    foot1.tendancy = tendancy;
    foot2.tendancy = tendancy;
    hand1.tendancy = tendancy;
    hand2.tendancy = tendancy;
    if (tendancy == -2) {
        // Slash flesh sound
        audio_play_sound(snd_slashflesh, 9, false);
        instance_destroy();
    }
    tendancy = -1;
}


</string>
