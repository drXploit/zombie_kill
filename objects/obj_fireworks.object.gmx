          <argument>
            <kind>1</kind>
            <string>/// Fireworks system
global.instances++;

// Particle system
part_fireworks_sys = part_system_create();

// System depth
part_system_depth(part_fireworks_sys, -8001);

// Rocket particles
part_rocket = part_type_create();
part_type_direction(part_rocket, 80, 100, 0, 0);
part_type_speed(part_rocket, 15, 20, 0, 0);
part_type_life(part_rocket, room_speed*1.1, room_speed*1.3);
part_type_gravity(part_rocket, 0.15, 270);
part_type_alpha1(part_rocket, 0);

// Emitter
part_fireworks_emit = part_emitter_create(part_fireworks_sys);
part_emitter_region(part_fireworks_sys, part_fireworks_emit, x-100, x+100, y+100, y-25, ps_shape_line, ps_distr_linear);
part_emitter_stream(part_fireworks_sys, part_fireworks_emit, part_rocket, -20);

// Trail particles
part_trail = part_type_create();
part_type_color2(part_trail, c_red, c_orange);
part_type_gravity(part_trail, 0.1, 270);
part_type_shape(part_trail, pt_shape_cloud);
part_type_life(part_trail, room_speed*0.5, room_speed*0.6);
part_type_alpha2(part_trail, 0.8, 0.0);
part_type_size(part_trail, 0.3, 0.4, -0.01, 0);

// Explosion particles
part_explode = part_type_create();
part_type_gravity(part_explode, 0.1, 270);
part_type_shape(part_explode, pt_shape_flare);
part_type_life(part_explode, 15, 40);
part_type_alpha2(part_explode, 1, 0);
part_type_size(part_explode, 0.5, 0.7, -0.001, 0);
part_type_direction(part_explode, 0, 359, 0, 0);
part_type_speed(part_explode, 2, 10, 0, 0);
part_type_blend(part_explode, true);
part_type_color_rgb(part_explode,0, 255, 0, 255, 0, 255);

// Play fireworks sound for 30 seconds
audio_play_sound(snd_fireworks, 4, false);

// Timer to destroy after 30 seconds
alarm[0] = room_speed*30;

</string>
